    "@typescript-eslint/parser": "^7.5.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.3.0",
    "turbo": "^1.12.0",
    "typescript": "^5.4.0"
